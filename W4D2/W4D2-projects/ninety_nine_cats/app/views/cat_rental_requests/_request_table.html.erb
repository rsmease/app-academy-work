<% cat_requests = CatRentalRequest.where(cat_id: cat[:id]) %>

<table>
  <tr>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Status</th>
  </tr>
<% cat_requests.each do |request| %>
  <tr>
    <td> <%= request.start_date %> </td>
    <td> <%= request.end_date %> </td>
    <td> <%= request.status %> </td>
  </tr>
<% end %>

</table>
